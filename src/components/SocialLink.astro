---
const { name, url, icon } = Astro.props;
---

<li>
    <div class="flex flex-row items-center mb-2">
        <img
            src={icon}
            alt="Profile"
            class="w-4 h-4 rounded-full object-cover mr-2"
        /><a href={url} target="_blank" class="text-sm text-primary hover:text-cyan-500">{name}</a>
    </div>
</li>
